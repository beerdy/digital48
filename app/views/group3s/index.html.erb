<p id="notice"><%= notice %></p>

<h1>Listing Group3s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Slave</th>
      <th>Image uid</th>
      <th>Image name</th>
      <th>Url</th>
      <th>Sort</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @group3s.each do |group3| %>
      <tr>
        <td><%= group3.title %></td>
        <td><%= group3.description %></td>
        <td><%= group3.slave %></td>
        <td><%= group3.image_uid %></td>
        <td><%= group3.image_name %></td>
        <td><%= group3.url %></td>
        <td><%= group3.sort %></td>
        <td><%= link_to 'Show', group3 %></td>
        <td><%= link_to 'Edit', edit_group3_path(group3) %></td>
        <td><%= link_to 'Destroy', group3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group3', new_group3_path %>
